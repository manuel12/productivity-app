{
  "name": "productivity-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@hookform/resolvers": "^3.3.4",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.48",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "axios": "^1.8.1",
    "chart.js": "^4.4.0",
    "cypress-plugin-steps": "^1.1.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "react-router-dom": "^6.16.0",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4",
    "yup": "^1.3.3",
    "yup-password": "^0.2.2"
  },
  "scripts": {
    "test:chrome": "cypress run --browser chrome",
    "test:smoke-chrome": "npx cypress run --spec 'cypress/tests/smoke/smoke-tests.cy.js' --browser chrome",
    "test:firefox": "cypress run --browser firefox",
    "test:smoke-firefox": "cypress run --spec 'cypress/tests/smoke/smoke-tests.cy.js' --browser firefox",
    "test:edge": "cypress run --browser edge",
    "test:smoke-edge": "npx cypress run --spec 'cypress/tests/smoke/smoke-tests.cy.js' --browser edge",
    "test:electron": "cypress run --browser electron",
    "test:smoke-electron": "cypress run --spec 'cypress/tests/smoke/smoke-tests.cy.js' --browser electron",
    "test:all-browsers": "npm run test:chrome && npm run test:firefox && npm run test:edge && npm run test:electron",
    "test:a11y": "cypress run --spec 'cypress/tests/a11y/'",
    "test:api": "cypress run --spec 'cypress/tests/api/'",
    "test:e2e": "cypress run --spec 'cypress/tests/e2e/'",
    "test:functional": "cypress run --spec 'cypress/tests/functional/'",
    "test:responsiveness": "cypress run --spec 'cypress/tests/responsiveness/",
    "test:smoke": "cypress run --spec 'cypress/tests/smoke/smoke-tests.cy.js'",
    "test:smoke-all-browsers": "npm run test:smoke-chrome && npm run test:smoke-firefox && npm run test:smoke-edge && npm run test:smoke-electron",
    "test:qase:e2e": "cypress run --spec 'cypress/tests/qase/e2e/**/*.cy.{js,jsx,ts,tsx}' -C qase.config.js",
    "test:open": "cross-env NODE_ENV=prod cypress open",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@simonsmith/cypress-image-snapshot": "^8.1.2",
    "cross-env": "^7.0.3",
    "cypress": "^13.2.0",
    "cypress-mochawesome-reporter": "^3.8.2",
    "cypress-multi-reporters": "^2.0.5",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "wick-a11y": "^2.0.1"
  }
}
